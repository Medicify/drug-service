generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model drugs {
  id                String  @id @default(dbgenerated("(uuid())")) @db.VarChar(32)
  image             String? @db.VarChar(255)
  title             String? @db.VarChar(255)
  product_url       String? @unique(map: "product_url") @db.VarChar(255)
  description       String? @db.Text
  indication        String? @db.Text
  compotition       String? @db.VarChar(255)
  dose              String? @db.Text
  how_to_use        String? @db.VarChar(255)
  attention         String? @db.Text
  indication_contra String? @db.Text
  side_effect       String? @db.Text
  product_class     String? @db.VarChar(255)
  package           String? @db.VarChar(255)
  manufactur        String? @db.VarChar(25)
  bpom              String? @db.VarChar(255)
}
